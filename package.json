{
  "name": "ha-charging-station-control",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev:frontend": "next dev",
    "dev:tasks": "bun src/server/tasks/index.ts",
    "dev": "concurrently \"bun src/server/tasks/index.ts\" \"next dev\"",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "concurrently \"bun src/server/tasks/index.ts\" \"next start -p 80\"",
    "seed": "test -f ./prisma/db.sqlite && rm ./prisma/db.sqlite; touch ./prisma/db.sqlite && bun run db:push && bun run ./prisma/seeder.ts",
    "seed:prod": "bun run ./prisma/seeder.ts"
  },
